<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lostandfoundapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LostAndFoundApp">
        <activity
            android:name=".ContainerAccessActivity"
            android:label="@string/title_activity_container_access"
            android:theme="@style/Theme.LostAndFoundApp.NoActionBar"></activity>
        <activity
            android:name=".CustomerLoginActivity"
            android:label="@string/title_activity_customer_login"
            android:theme="@style/Theme.LostAndFoundApp.NoActionBar" />
        <activity
            android:name=".RegistractionActivity"
            android:label="@string/title_activity_registraction"
            android:theme="@style/Theme.LostAndFoundApp.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>